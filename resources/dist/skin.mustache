<div id="ol-app" data-json="{{data-json}}">
		<div class="banner">
		<div>
			<ul class="skin-alexandria-notifications">{{{data-portlets.data-notifications.html-items}}}</ul>
		</div>
	</div>
		<header id="header-bar" class="header-bar">
		<div class="hamburger-component checkbox-menu">
			<li class="dropdown-menu">
		<label for="more">
			{{ label }}
		</label>
		<input id="more" type="checkbox" class="navigation-component__checkbox">
		<div class="navigation-dropdown-component">
			
		
				<ul>{{{alex-sidebar}}}</ul></div>
	</li>
		</div>

		<div class="logo-component">
			<a href="{{link-mainpage}}" title="{{msg-tagline}}">
				<div class="logo-txt">
					{{#alex-data-wordmark.src}}<img class="logo-icon" src="{{alex-data-wordmark.src}}" alt="{{msg-sitetitle}}" width="{{alex-data-wordmark.width}}" height="{{alex-data-wordmark.height}}">{{/alex-data-wordmark.src}}
					{{^alex-data-wordmark.src}}<h1>{{msg-sitetitle}}</h1>{{/alex-data-wordmark.src}}
				</div>
			</a>
		</div>

		<div class="search-component">
			<form class="alex-fallback-search" action="{{data-search-box.form-action}}">
		<div>{{{data-search-box.html-input}}}</div>
		<div>{{{data-search-box.html-button-search-fallback}}}</div>
		<input type="hidden" name="title" value="{{data-search-box.page-title}}">
	</form>
		</div>

		<div class="account-component checkbox-menu">
			<li class="dropdown-menu">
		<label for="p-personal">
			{{ label }}
		</label>
		<input id="p-personal" type="checkbox" class="navigation-component__checkbox">
		<div class="navigation-dropdown-component">
			
		
				<ul>{{{data-portlets.data-user-menu.html-items}}}</ul></div>
	</li>
		</div>
	</header>
		<article class="alexandria-article">
		<header>
			<div class="alexandria-article__tagline">
				{{msg-tagline}}
			</div>
			<h1>{{{html-title}}}</h1>
			<div class="article__subtitle">{{{html-subtitle}}}</div>
			<ul class="alexandria-article__lastmod">{{{alex-lastmod}}}</ul>
			<div id="{{id}}" class="alexandria-portlet">
		<ul>{{{data-portlets.data-views.html-items}}}</ul>
	</div>
		</header>
		<div>{{{html-site-notice}}}</div>
		<main>{{{html-body-content}}}</main>
	</article>
		<footer>
		<div class="footer__content">
			<div class="footer__menus">
				<div class="footer__menu">
		<h2>{{msg-sitetitle}}</h2>
		<ul>
					{{#data-footer.data-places.array-items}}<li id="{{id}}">{{{html}}}</li>{{/data-footer.data-places.array-items}}</ul>
	</div>
			</div>
			<hr>
			<div class="footer__end">{{{alex-footer-icons}}}</div>
		</div>
	</footer>
	</div>
